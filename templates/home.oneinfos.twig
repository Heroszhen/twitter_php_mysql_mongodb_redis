{% extends "base.twig" %}

{% block content %}
<div id="home" class="container pt-3">
    <div class="row justify-content-center">
        <h1 class="col-md-8 h2 mb-4">{{infos.title}}</h1>
        <div class="col-md-8">
            {{infos.content|raw}}
            <div class="text-right">
                {{infos.created|date("d/m/Y")}}
            </div>
        </div>
    </div>
</div>

{% endblock %}